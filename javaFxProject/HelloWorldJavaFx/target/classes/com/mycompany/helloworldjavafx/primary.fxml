<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="222.0" prefWidth="360.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.helloworldjavafx.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label fx:id="lblMsg" text="message" textFill="#d71e1e">
         <font>
            <Font name="Corbel Bold" size="27.0" />
         </font>
      </Label>
      <Button fx:id="btnClick" mnemonicParsing="false" onAction="#clicouBotao" text="Click me" />
      <Button fx:id="btnPress" mnemonicParsing="false" onAction="#clickButton" prefHeight="111.0" prefWidth="158.0" text="dont click me" />
   </children>
</VBox>
